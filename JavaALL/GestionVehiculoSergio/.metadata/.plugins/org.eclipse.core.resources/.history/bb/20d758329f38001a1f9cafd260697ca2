package model;

import java.io.Serializable;

public class Persona implements Serializable {
	private String name;
	private String gender;
	private Integer age;
	private Double peso;
	private Boolean isInVehicule;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getGender() {
		return gender;
	}
	public void setGender(String gender) {
		this.gender = gender;
	}
	public Integer getAge() {
		return age;
	}
	public void setAge(Integer age) {
		this.age = age;
	}
	public Double getPeso() {
		return peso;
	}
	public void setPeso(Double peso) {
		this.peso = peso;
	}
	public Boolean getIsInVehicule() {
		return isInVehicule;
	}
	public void setIsInVehicule(Boolean isInVehicule) {
		this.isInVehicule = isInVehicule;
	}
}
